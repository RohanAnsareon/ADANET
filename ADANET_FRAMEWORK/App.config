<?xml version="1.0" encoding="utf-8" ?>
<configuration>

  <connectionStrings>
    <add name="Default"
         connectionString="Server=(LocalDb)\MSSQLLocalDB;Database=BlaBlaMart;Trusted_Connection=True;"
         providerName="System.Data.SqlClient"/>
  </connectionStrings>

	<appSettings>
		<add key="InsertUser" value="INSERT INTO [dbo].[User]
                                                        ([Name]
                                                        ,[Age])
                                                  VALUES
                                                        (@Name
                                                        ,@Age);"/>
	</appSettings>
	
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.1" />
  </startup>
</configuration>